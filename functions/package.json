{
  "name": "firestore-algolia-search",
  "version": "0.5.5",
  "description": "Index Cloud Firestore collection to Algolia",
  "scripts": {
    "build": "npm run clean && npm test && npm run compile",
    "clean": "rimraf lib",
    "compile": "tsc",
    "import": "ts-node src/import/index.ts",
    "lint": "pretty-quick --staged && lint-staged",
    "prepare": "npm run build",
    "test": "jest --coverage"
  },
  "bin": "cli.js",
  "hooks": {
    "pre-commit": "npm run lint"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@algolia/client-common": "^4.8.6",
    "@algolia/transporter": "^4.8.6",
    "algoliasearch": "^4.8.6",
    "firebase-admin": "^9.8.0",
    "firebase-functions": "^3.14.1",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^27.0.2",
    "@types/js-yaml": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.1.0",
    "firebase-functions-test": "^0.2.3",
    "jest": "^27.2.5",
    "js-yaml": "^3.14.1",
    "mocked-env": "^1.3.2",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^9.1.1",
    "typescript": "^3.8.0"
  },
  "engines": {
    "node": ">=15.0.0"
  }
}
